
@using TepayLink.Sdisco.Web.Areas.Admin.Models.Common.Modals
@using TepayLink.Sdisco.Web.Areas.Admin.Models.RevenueByMonths
@model RevenueByMonthViewModel

@await Html.PartialAsync("~/Areas/Admin/Views/Common/Modals/_ModalHeader.cshtml", new ModalHeaderViewModel(L("Details")))

<div class="modal-body">
    <div id="RevenueByMonthInformationsTab">
        
        <div class="row">
            <div class="col-6 mb-3">
                <h6>@L("UserId") :</h6>
                @Model.RevenueByMonth.UserId
            </div>
            <div class="col-6 mb-3">
                <h6>@L("Revenue") :</h6>
                @Model.RevenueByMonth.Revenue
            </div>
            <div class="col-6 mb-3">
                <h6>@L("Date") :</h6>
                @Model.RevenueByMonth.Date
            </div>


        </div>

    </div>
</div>

@await Html.PartialAsync("~/Areas/Admin/Views/Common/Modals/_ModalFooterWithClose.cshtml")

